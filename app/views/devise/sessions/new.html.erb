<%= render "devise/shared/links" %>




<div class="container">
  <div class="frame">
    <div class="nav">
      <ul class"links">
        <li class="signin-active"><a class="btn">Sign in</a></li>
      </ul>
    </div>
    <div ng-app ng-init="checked = false">
    
                <form class="form-signin" action="" method="post" name="form">
                <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

                    <label for="username">Email</label>
                    <%= f.email_field :email, autofocus: true , class: "form-styling" do %><% end %>

                    <label for="password">Password</label>
                    <%= f.password_field :password, autocomplete: "off", class: "form-styling" do %> <% end %>

                    <%# if devise_mapping.rememberable? -%>
                    <!-- <input type="checkbox" id="checkbox"/>
                    <label for="checkbox" ><span class="ui"></span>Remember Me</label>
                     <%#= f.check_box :remember_me , id: "checkbox" do %> Remember<%# end %>
                     <%# end -%> -->


                    <div class="btn-animate actions">
                       <%= f.submit "Sign In", class: "btn-signin" do  %>
                       Sign In <% end %>
                    </div>

                    <p class="notice notice-msg"><%= notice %></p>
                    <p class="alert alert-msg"><%= alert %></p> 


                    <% end %>

                    
                 </form>
           
    </div>
      
      
     
  </div>
    

</div>
</div>