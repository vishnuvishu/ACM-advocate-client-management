<div class="container-fluid">


<div class="main-heading">
       <h1> All Additional Mobiles</h1>
</div>
    


<div class="main-heading-body"> 
<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Mobile</th>
      <th>Client</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @all_mobiles.each do |all_mobile| %>
      <tr>
        <td><%= all_mobile.mobile %></td>
        <td><%= all_mobile.client.name %></td>
 <!--   <td><%#= link_to all_mobile, class: "btn btn-success" do %> Show <%# end %></td>
        <td><%#= link_to edit_all_mobile_path(all_mobile), class: "btn btn-info" do %>Edit <%# end %></td>
        <td><%#= link_to all_mobile, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger" do %>Delete<%# end %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<%#= link_to 'New All mobile', new_all_mobile_path %>
</div>
</div>